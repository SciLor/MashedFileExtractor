<Window x:Class="SciLors_Mashed_File_Extractor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SciLor's Mashed File Extractor" Height="536" Width="1043" Name="wndMain">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="301*" />
            <RowDefinition Height="10*" />
        </Grid.RowDefinitions>
        <StatusBar Height="23" Name="stbMainStatus" VerticalAlignment="Bottom" Grid.RowSpan="2" />
        <Menu Height="19" Name="mnuMain" VerticalAlignment="Top">
            <MenuItem Name="mniFile" Header="File">
                <MenuItem Header="Open" Name="mniOpen" Click="mniOpen_Click" />
                <MenuItem Header="Exit" Name="mniExit" />
            </MenuItem>
        </Menu>
        <DockPanel Margin="0,25,0,17" Name="dckMain">
            <TreeView Name="tvwContent" Width="330" HorizontalAlignment="Left">
            </TreeView>
        </DockPanel>
    </Grid>
</Window>
